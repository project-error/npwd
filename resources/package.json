{
  "name": "npwd-game",
  "version": "0.1.0",
  "description": "Resources configuration",
  "main": "index.js",
  "license": "CC-BY-NC-SA-4.0",
  "private": false,
  "repository": "https://github.com/project-error/new-phone-who-dis",
  "scripts": {
    "build:client": "esbuild client/client.ts --bundle --minify --outfile=dist/client.js",
    "build:server": "esbuild server/server.ts --bundle --platform=node --outfile=dist/server.js",
    "build": "yarn build:server && yarn build:client",
    "build:release": "cross-env IS_SENTRY=true webpack --mode production",
    "watch:client": "esbuild client/client.ts --bundle --watch --outfile=dist/client.js",
    "watch:server": "esbuild server/server.ts --bundle --platform=node --watch --outfile=dist/server.js",
    "watch": "yarn watch:server && yarn watch:client",
    "test": "jest",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@discordjs/collection": "^0.2.4",
    "@project-error/pe-utils": "^0.1.6",
    "axios": "^0.21.4",
    "dayjs": "^1.10.7",
    "fivem-js": "^1.3.1",
    "md5": "^2.3.0",
    "mysql2": "^2.3.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@citizenfx/client": "^2.0.4170-1",
    "@citizenfx/server": "^2.0.4167-1",
    "@sentry/integrations": "^6.15.0",
    "@sentry/node": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "@sentry/webpack-plugin": "^1.18.3",
    "@types/jest": "^26.0.14",
    "@types/md5": "^2.2.1",
    "@types/node": "^14.17.34",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.0.3",
    "esbuild": "^0.14.11",
    "eslint": "^7.32.0",
    "jest": "^26.5.3",
    "remove-files-webpack-plugin": "^1.4.3",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.5.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
