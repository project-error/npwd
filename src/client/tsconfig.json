{
  "compilerOptions": {
    "types": ["@citizenfx/client", "@types/node"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "moduleResolution": "Node16",
    "target": "ES6",
    "allowJs": true,
    "lib": ["ES2015", "ES2016", "ES2017", "ES2018", "ES2019", "ES2020", "ES2021"]
  },
  "include": ["*.ts", "../shared/**/*.ts", "services/AnimationService.ts", "services/PhoneService.ts"]
}
