{
  "compilerOptions": {
    "target": "es5",
    "downlevelIteration": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "inlineSources": true,
    "sourceRoot": "/",
    "noFallthroughCasesInSwitch": true,
    "jsx": "preserve"
  },
  "include": ["src"]
}
